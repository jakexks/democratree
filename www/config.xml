<?xml version='1.0' encoding='utf-8'?>
<widget id="uk.co.democratree" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Democratree</name>
    <description>
        An application to determine the best location to plant 1000 trees in Bristol, using crowdsourced votes.
    </description>
    <author email="js9938@my.bristol.ac.uk" href="http://democratree.co.uk">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
</widget>
